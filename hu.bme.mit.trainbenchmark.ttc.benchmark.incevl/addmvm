#!/bin/bash
mvn install:install-file -Dfile=./epsilon-1.2-core.jar -DgroupId=org.eclipse.epsilon -DartifactId=org.eclipse.epsilon.core -Dversion=1.2.0 -Dpackaging=jar
mvn install:install-file -Dfile=./org.eclipse.epsilon.evl.incremental.jar -DgroupId=org.eclipse.epsilon -DartifactId=org.eclipse.epsilon.evl.incremental -Dversion=0.0.1 -Dpackaging=jar
mvn install:install-file -Dfile=./org.eclipse.epsilon.custom.jar -DgroupId=org.eclipse.epsilon -DartifactId=org.eclipse.epsilon.custom -Dversion=0.0.1 -Dpackaging=jar
